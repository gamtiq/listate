{
    "presets": [
        "es2015"
    ],
    "env": {
        "commonjs": {
            "plugins": [
                ["transform-es2015-modules-commonjs", {
                    "loose": true
                }]
            ]
        },
        "umd": {
            "plugins": [
                ["transform-es2015-modules-umd", {
                    "loose": true
                }]
            ]
        }
    },
    "sourceMaps": true
}
